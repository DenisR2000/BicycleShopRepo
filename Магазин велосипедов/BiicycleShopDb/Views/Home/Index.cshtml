@model IEnumerable<BiicycleShopDb.Models.Bicycle>
@using ReflectionIT.Mvc.Paging
@addTagHelper *,ReflectionIT.Mvc.Paging
@{ 
}

<header class="card-header">
    <div class="w-100 d-flex flex-row-reverse align-content-center; align-items-center justify-content-between" style="height: 130px; padding: 20px; margin: 0; color: white; font-family: sans-serif; font-size: 20px; background: rgba(0,0,0,0.5);">
        <div class="d-flex flex-row-reverse align-items-center" style="margin-left: 8px">
            <div style="height: 80px; width: 80px; border-radius: 50%; background-color: none; margin-left: 9px;">
                <img style="width: 100%; border-radius: 50%;" src="data:image/jpeg;base64," alt="" />
            </div>
            @if (User.Identity.Name != null)
            {
                <span>@User.Identity.Name</span>
            }
            else
            {
                <span>UserName</span>
            }
        </div>
        <div class="align-items-start">
            @using (Html.BeginForm("Logout", "Account"))
            {
                <input type="submit" value="Logout" style="color: white; width: 80px; font-family:sans-serif;" class="btn Registr_btn"/>
            }
        </div>
    </div>
    @* <div class="align-self-center" style="height: 30%;">
            <h1 class="header_title" style="color: white; font-family: sans-serif; margin: 20px 10px">Welcome to our website!</h1>
            <div class="header_text" style="font-family: sans-serif; font-size: 16px; margin-left: 10px">We offer a wide range of bicycles for every taste!</div>
        </div>
        <div style="height: 70%;" class="w-100 d-flex flex-row justify-content-between align-items-end">
            <a asp-action="Index" asp-area="" asp-controller="Home" style="color: white; width: 80px; font-family:sans-serif;" class="btn home_btn">Home</a>
            <a asp-action="index" asp-area="" asp-controller="Admin" style="color: white; width: 80px; font-family: sans-serif; " class="btn admin_btn">Admin</a>
        </div>
        <div>
            <a href="" style="color: white; width: 80px; font-family:sans-serif;" class="btn login_btn">Login</a>
        </div>*@
    <div class="d-flex flex-column flex-wrap w-100 h-100">
        <div style="height: 400px; margin: 10px 20px; margin-top: 100px;">
            <h1 class="header__title _anim-items anim-no-height">Welcome to our website!</h1>
            <div class="header__text _anim-items anim-no-height">We offer a wide range of bicycles for every taste!</div>
        </div>
        <div class="d-flex flex-row justify-content-between">
            <div style="height: 70%;" class="w-100">
                <a asp-action="Index" asp-area="" asp-controller="Home" style="color: white; width: 80px; font-family:sans-serif;" class="btn home_btn">Home</a>
                <a asp-action="index" asp-area="" asp-controller="Admin" style="color: white; width: 80px; font-family: sans-serif; " class="btn admin_btn">Admin</a>
            </div>
            <div>
                <a href="~/Cart/Index" style="color: white; width: 100px; height: 40px; font-family: sans-serif;" class="btn My_Cart_btn">My cart</a>
            </div>
            <a href="~/Account/Login" style="color: white; width: 80px; font-family:sans-serif;" class="btn login_btn">Login</a>
        </div>
    </div>
</header>

<div class="d-flex flex-row flex-wrap align-content-center justify-content-center align-items-center">
    <h2 class="visible_ __visible__text">@ViewBag.Message_By</h2>
</div>

<div class="d-flex flex-row flex-wrap align-content-center justify-content-center align-items-center">

    @foreach (var bicicle in Model)
    {
        <div class="card m-3 p-3" style="width: 18rem;">
            <img src="@bicicle.Img_Url" class="card-img-top" alt="...">
            <div class="card-body">
                <h5 class="card-title">@bicicle.Title</h5>
                <div class="card-text d-flex flex-column">
                    <div class="d-flex flex-row">
                        <span>Number speeds: </span>
                        <span>@bicicle.Number_Speeds</span>
                    </div>
                    <div class="d-flex flex-row">
                        <span>Prise: </span>
                        <span class="text-danger">@bicicle.Prise</span>
                    </div>
                </div>
               @* <a href="~/Home/Buy/@bicicle.BicycleId" class="btn btn-outline-success" style="width: 100%">
                    Card
                    <img style="width: 10%;" src="https://pngicon.ru/file/uploads/korzina-1.png" alt="" />
                </a>*@
            <div style="width: 100%">
                @using (Html.BeginForm("AddToCart", "Cart"))
                {
                    <input type="hidden" name="BicycleId" value="@bicicle.BicycleId" />
                    @Html.Hidden("returnUrl", Context.Request.Path.Value);
                    <input type="submit" style="width: 100%;" value="Add to card" class="btn btn-success" />
                } 
                @*<a style="width: 100%;" href="~/Info/Index/@bicicle.BicycleId" class="btn btn-warning">Info</a>*@            </div>
            </div>
        </div>
    }
</div>


<nav aria-label="Bicycle">
    <div class="d-flex flex-row align-items-center justify-content-center align-content-center">
        @await this.Component.InvokeAsync("Pager", new { pagingList = this.Model })
    </div>
</nav>
